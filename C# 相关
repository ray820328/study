FileStream fs = File.Create(outputFile);
                BinaryFormatter bf = new BinaryFormatter();
                byte[] array = Encoding.ASCII.GetBytes(strContent.ToString());
                MemoryStream stream = new MemoryStream(array);
                bf.Serialize(fs, stream);
                fs.Close();
                
                
